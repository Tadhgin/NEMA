{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@backend/*": [
        "backend/*"
      ]
    },
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
